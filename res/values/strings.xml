<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title for the search suggest provider. -->
    <string name="global_search">Android Search</string>

    <!-- Do not translate.  This string is not displayed in UI (because we have a badge icon). -->
    <string name="search_label" translatable="false">Google</string>

    <!-- This is the hint text shown in the system search dialog for global search, 
         before text is entered. -->
    <string name="search_hint">Android Search</string>

    <!-- Note that this is the standard suggest url.  It uses the current locale for language -->
    <!-- (t%1$s) and country (%2$s) and should not need to be replaced by locale or mcc -->
    <!-- selected resources. -->
    <!-- Any changes to this (especially in terms of adding/removing the MCC-specific flavors) -->
    <!-- should be mirrored by changes in apps/Browser -->
    <!-- Do not translate.  This string is an internal URI, not seen in the UI. -->
    <string name="google_search_base" translatable="false">
        http://www.google.com/complete/search?hl=%1$s&amp;gl=%2$s&amp;</string>

    <!-- Format of a contact item's description line. For e.g. "m: 6509999999". -->
    <string name="contact_details_format">%1$s %2$s</string>
    <string name="contact_phone_type_mobile">m:</string>
    <string name="contact_phone_type_home">h:</string>
    <string name="contact_phone_type_work">w:</string>

    <!-- The title of the suggestion that is always shown at the end of a list of results to let
         the user search the web for the query currently in the text field. -->
    <string name="search_the_web_title">Search the web</string>
    
    <!-- The description of the suggestion that is always shown at the end of a list of results
         to let the user search the web for the query currently in the text field. -->
    <string name="search_the_web_description">for \'%1$s\'</string>
    
    <!-- The title of the suggestion that is shown at the end of a list of results if the query
         seems to match a url, to let the user go straight to the browser with that url. -->
    <string name="go_to_website_title">Go to website</string>
    
    <string name="more_results">More results...</string>
    
    <!-- A count of the number of results provided by a particular app to global search,
         to be displayed in the "More results..." list item.
         This string will be used when no results from the source were shown in the top-level
         promoted list of search suggestions, whereas the 'additional_result_count' string will be
         used when some results from the source were shown in the top-level list. -->
    <plurals name="total_result_count">
        <!-- Case of one result. -->
        <item quantity="one">%d result</item>
        
        <!-- Case of "few" (small number of) results. -->
        <item quantity="few">%d results</item>
        
        <!-- Case of a plural number of results. -->
        <item quantity="other">%d results</item>
    </plurals>

    <!-- A count of the number of additional results provided by a particular app to global search,
         that haven't been displayed in the promoted list, to be displayed in the "More results..."
         list item.
         This string will be used when some results from the source were shown in the top-level
         promoted list of search suggestions, whereas the 'total_result_count' string will be used
         when no results from the source were shown in the top-level list. -->
    <plurals name="additional_result_count">
        <!-- Case of one result. -->
        <item quantity="one">%d additional result</item>

        <!-- Case of "few" (small number of) results. -->
        <item quantity="few">%d additional results</item>

        <!-- Case of a plural number of results. -->
        <item quantity="other">%d additional results</item>
    </plurals>

    <!-- The separator used for distinguishing app names from their counts
         in the "More results..." list item of global
         search. For example, if it's a colon (:) followed by a space, then
         the final string might look like: "Apps: 1".
         Double quotes are needed around the string if it contains leading
         or trailing whitespace. -->
    <string name="result_count_count_separator">": "</string>

    <!-- The separator character used for distinguishing different apps
         and their result counts in the "More results..." list item of global
         search. For example, if it's a comma (,) followed by a space, then the
         final string might look like: "Apps: 1, Google: 4, Contacts: 8". -->
    <string name="result_count_app_separator">", "</string>

    <!-- Search settings stuff -->
    
    <!-- Search settings title -->
    <string name="search_settings">Search settings</string>
    
    <!-- Title for 'web search' category of search settings -->
    <string name="web_search_category_title">Web search</string>
    
    <!-- Title and summary for 'search engine' setting -->
    <string name="web_search_source">Search engine</string>
    <string name="web_search_source_summary">Choose a web search engine</string>
    
    <!-- Title and summaries for 'search engine settings' link (if available for the chosen
         web search source) -->
    <string name="search_engine_settings">Search engine settings</string>
    <string name="search_engine_settings_summary_enabled">Settings for web search engine \'%1$s\'</string>
    <string name="search_engine_settings_summary_disabled">No settings for web search engine \'%1$s\'</string>
    
    <!-- Title and summaries for 'show web suggestions' check box setting -->
    <string name="show_web_suggestions">Show suggestions</string>
    <string name="show_web_suggestions_summary">Show suggestions from the web search engine as you type</string>
    
    <!-- Title for 'system search' category of search settings -->
    <string name="system_search_category_title">Phone search</string>
    
    <!-- Title and summary for 'search sources' link for system search -->
    <string name="search_sources">What to search</string>
    <string name="search_sources_summary">Choose what to search on the phone</string>
    
    <!-- Summaries used for the check box settings for a given source within 'search sources' -->
    <string name="search_source_enabled">Search enabled</string>
    <string name="search_source_disabled">Search disabled</string>

    <!-- Title and summary used for the option to clear search shortcuts from the system
         search history -->
    <string name="clear_shortcuts">Clear search shortcuts</string>
    <string name="clear_shortcuts_summary">Clear shortcuts to recently chosen search results</string>
    
    <!-- Strings used in the dialog shown after choosing the 'clear search shortcuts' setting. -->
    <string name="clear_shortcuts_prompt">"This will clear all shortcuts to recently chosen search results."\n\n"(To clear your Browser history, use the Browser's settings.)"</string>
    <string name="agree">OK</string>
    <string name="disagree">Cancel</string>
        
    <!-- Music source stuff. TODO: This should be removed once we fix the music source. -->
    
    <!-- The description of a music track suggestion. -->
    <string name="music_track">Track</string>
    
    <!-- The description of a music album suggestion. -->
    <string name="music_album">Album</string>
    
    <!-- The description of a music artist suggestion. -->
    <string name="music_artist">Artist</string>

</resources>
